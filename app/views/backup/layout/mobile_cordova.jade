doctype html
html
    head
        meta( charset="utf-8" )
        meta( name="viewport" content="width=device-width, initial-scale=1.0" )
        
        meta(http-equiv="Content-Security-Policy" content="default-src 'self' data: gap: https://ssl.gstatic.com 'unsafe-eval'; style-src 'self' 'unsafe-inline'; media-src *; img-src 'self' data: content:;")
        meta( name="format-detection" content="telephone=no" )
        meta( name="msapplication-tap-highlight" content="no" )
        meta( name="viewport" content="user-scalable=no, initial-scale=1, maximum-scale=1, minimum-scale=1, width=device-width" )

        title NUGU - prototypes
        
        link( href="css/material-icons.css" rel="stylesheet" )
        link( href="css/materialize.css" type="text/css" rel="stylesheet" )
        link( href="css/prototype.css" type="text/css" rel="stylesheet" )
    body( class=( grayscale ? 'grayscale' : '' ) )
        include header_cordova

        block content
        
        block screen

        //- Lib
        script( src="js/gsap/src/minified/TimelineMax.min.js" )
        script( src="js/gsap/src/minified/TweenMax.min.js" )
        script( src="js/gsap/src/minified/plugins/CSSPlugin.min.js" )
        script( src="js/gsap/src/minified/plugins/ScrollToPlugin.min.js" )
        script( src="js/masonry/masonry.pkgd.min.js" )
        script( src="js/jquery/dist/jquery.min.js" )
        script( src="js/materialize/dist/js/materialize.min.js" )
        script( src="js/A4B.Lib.min.js" )
        //- script( src="/javascripts/A4B.Lib.js" )
        script( src="js/prototype.js" )
        //- script( src="javascripts/speech.js" )
            - if ( title == 'Masonry' )
                 script( src="/bower_components/masonry/masonry.pkgd.min.js" )
                 script( src="/javascripts/masonry.js" )